     type_code : 70
     p_codes :
      -
        code : |1-
         PROCEDURE MaFenêtre()
         
         hauteur_chaine est entier
      -
        code : |1-
         si hauteur_chaine=0 ALORS
         	LIB_NB_BLOC=""
          	LIB_DATE_HALVING = ""
         	
         	retour
         FIN
         
         // calcul du bloc de halving
         num_bloc_prochain_halving est entier =  (hauteur_chaine -modulo(hauteur_chaine, 210 000)) + 210 000
         nb_bloc_restant est un entier = num_bloc_prochain_halving - hauteur_chaine
         
         // calcul de la date
         dureée_restante est durée 
         dureée_restante.EnMinutes = nb_bloc_restant *10
         date_estimée est date = Maintenant() 
         date_estimée += dureée_restante
         
         
         LIB_NB_BLOC = nb_bloc_restant
         LIB_DATE_HALVING = date_estimée
        type : 234
      -
        code : |1-
         
        type : 17
