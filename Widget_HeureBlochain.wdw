      -
        code : |1+
         PROCEDURE MaFenêtre()
         
         dernier_bloc_header est json
         
         // rafraichissement auto toutes les 1 secondes
         TimerSys(()=>{ DemandeMiseAJourUI() },1s)
      -
        code : |1-
         // has du dernier block
         //hash_dernier_bloc est BlockHash = gclNoeud.getbestblockhash()
         //bloc_info est json = gclNoeud.getblockheader_json(hash_dernier_bloc)
         
         SI dernier_bloc_header.time=null alors 
         	 LIB_HEURE = ""
         	 LIB_HASH  = ""
         	 retour
         FIN
         hash_dernier_bloc est BlockHash 
         hash_dernier_bloc.valeur = ""+dernier_bloc_header.hash
         date_heure_bloc est une dateheure = EpochVersDateHeure(dernier_bloc_header.time)
         now est un dateheure = maintenant() 
         duree est une durée = now - date_heure_bloc
         
         //LIB_HEURE = date_heure_bloc.versChaîne("HH:MM")
         
         LIB_HEURE = DuréeVersChaîne( duree ,"HH:MM:SS")
         hash_en_chaine est chaine = gauche(hash_dernier_bloc.valeur, 4) + "..." + droite(hash_dernier_bloc.valeur, 12)
         LIB_HASH  = hash_en_chaine
        type : 234
