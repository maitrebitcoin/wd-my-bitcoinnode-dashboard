     type : 2
  procedures :
   -
     name : _onNouveuBlockValide
     procedure_id : 1454201322849443452
     type_code : 14
     code : |1+
      // appelé si un nouveau bloc valide a  été ajouté a la fin de la chaine 
      procédure _onNouveuBlockValide(block est BlockHash )
      
      // récup des infos de ce bloc
      bloc_header est json = gclNoeud.getblockheader_json(block)
      
      //TDB_SansNom1.Affiche()
      pour i = 1 a TDB_SansNom1..Occurrence
      	widget est champ <- TDB_SansNom1[i]
      	// notifier le widget
      	QUAND EXCEPTION dans
      		widget.onNouveuBlockValide(bloc_header)
      	faire
      		// ignorer si la procédure n'existe pas
      	FIN
      FIN
     type : 458752
