      PROCEDURE MaFenêtre()
   -
     code : |1+
      
      
      //  vérif auto toutes les 3 secondes si un nouveau bloc est arrivé
      dernier_block_connu est BlockHash
      TimerSys(()=>{ 
      	dernier_block est BlockHash = gclNoeud.getbestblockhash()
      	si dernier_block.valeur <>	dernier_block_connu.valeur ALORS
      		dernier_block_connu = dernier_block
      		_onNouveuBlockValide(dernier_block)
      	FIN
      },1s)
     type : 34
